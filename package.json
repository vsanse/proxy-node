{
  "name": "proxy-node",
  "version": "1.0.0",
  "description": "Express proxy server that attaches cookies to API requests",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "start:node": "node index.js",
    "dev": "nodemon --watch . --ext js,json index.js",
    "remote": "nodemon remote.js",
    "remote:node": "node remote.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}
